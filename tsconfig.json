{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023"],
    
    /* Modules */
    "module": "node16",

    /* Emit */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,

    /* Completeness */
    "skipLibCheck": true
  }
}
